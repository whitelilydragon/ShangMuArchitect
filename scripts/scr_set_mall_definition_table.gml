while(ratio_page <= o.display_page_two)
    {
        //Get Index Of Tile
        ratio_index = o.display_tile_item[ratio_page,ratio_iterate]
        //Get Ratio Of Tile
        o.display_tile_ratio[ratio_page,ratio_iterate] = 32/background_get_width(ratio_index)
        if background_get_width(ratio_index) < background_get_height(ratio_index)
            display_tile_ratio[ratio_page,ratio_iterate] = 32/background_get_height(ratio_index);
        //Set Ratio Of Tile
        if ratio_page = 0 o.display_tile_menu_ratio[ratio_iterate] = o.display_tile_ratio[ratio_page,ratio_iterate];
        //Iterate Loop Counter
        ratio_iterate += 1
        //Iterate Page Counter
        if ratio_iterate > 19 {ratio_iterate = 0; ratio_page += 1;}
    }
use_item_aspect_ratio = display_tile_menu_ratio[0]

//Tile Menu Icon Y Offsets (Pages 1-12)
o.offset_iterate = 0;
o.offset_index = 0;
o.offset_x = 0
o.offset_y = 0;
o.offset_page = 0;
o.offset_tile_menu_icons_y[0] = 0
o.offset_tile_menu_icons_y_1[0,0] = 0

while(o.offset_page <= o.display_page_two)
    {
        //Get Index Of Tile
        o.offset_index = o.display_tile_item[o.offset_page,o.offset_iterate]
        //Set Offset Of Tile
        var use_aspect_ratio = 32/background_get_width(o.offset_index);
        o.offset_tile_menu_icons_x_1[o.offset_page,o.offset_iterate] = 0
        o.offset_tile_menu_icons_y_1[o.offset_page,o.offset_iterate] = (use_aspect_ratio*background_get_width(o.offset_index))/4;
        //Clear Offsets If Image X/Y Dimensions Are Too Similar
        var aspect_proportions = background_get_width(o.offset_index)/background_get_height(o.offset_index);
        if background_get_width(o.offset_index) = background_get_height(o.offset_index)
        or (aspect_proportions > 0.80 and aspect_proportions < 1.20)
            {
                o.offset_tile_menu_icons_x_1[o.offset_page,o.offset_iterate] = 0
                o.offset_tile_menu_icons_y_1[o.offset_page,o.offset_iterate] = 0;
            }

        if o.offset_page = 0 o.offset_tile_menu_icons_y[o.offset_iterate] = o.offset_tile_menu_icons_y_1[o.offset_page,o.offset_iterate];
        //Iterate Loop Counter
        o.offset_iterate += 1
        //Iterate Page Counter
        if o.offset_iterate > 19 {o.offset_iterate = 0; o.offset_page += 1;}
    }
o.use_item_offset_y = o.offset_tile_menu_icons_y[0]

//Tile Menu Icon X Offsets (Pages 1-12)
var offset_iterate = 0;
var offset_index = 0;
var offset_x = 0
var offset_y = 0;
var offset_page = 0;
o.offset_tile_menu_icons_x[0] = 0
o.offset_tile_menu_icons_x_1[0,0] = 0

while(offset_page <= o.display_page_two)
    {
        //Get Index Of Tile
        offset_index = o.display_tile_item[offset_page,offset_iterate]
        o.offset_tile_menu_icons_x_1[offset_page,offset_iterate] = 0
        //Set Offset Of Tile
        if background_get_width(offset_index) < background_get_height(offset_index)
            {
                var use_aspect_ratio = 32/background_get_height(offset_index);
                o.offset_tile_menu_icons_x_1[offset_page,offset_iterate] = (use_aspect_ratio*background_get_height(offset_index))/4;
                o.offset_tile_menu_icons_y_1[offset_page,offset_iterate] = 0
            }
        //Clear Offsets If Image X/Y Dimensions Are Too Similar
        var aspect_proportions = background_get_width(offset_index)/background_get_height(offset_index);
        if (background_get_width(offset_index) = background_get_height(offset_index))
        or (aspect_proportions > 0.80 and aspect_proportions < 1.20)
            {
                o.offset_tile_menu_icons_x_1[offset_page,offset_iterate] = 0
                o.offset_tile_menu_icons_y_1[offset_page,offset_iterate] = 0
            }

        if offset_page = 0 o.offset_tile_menu_icons_x[offset_iterate] = o.offset_tile_menu_icons_x_1[offset_page,offset_iterate];
        //Iterate Loop Counter
        offset_iterate += 1
        //Iterate Page Counter
        if offset_iterate > 19 {offset_iterate = 0; offset_page += 1;}
    }
o.use_item_offset_x = o.offset_tile_menu_icons_x[0]
