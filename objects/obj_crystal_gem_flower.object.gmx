            <kind>1</kind>
            <string>//Pause Safety Check
if obj_game_logic.pause = 0
{
//Advance Frames
if frame_direction = 0 frame+=0.15;
else                   frame-=0.15;
//Reverse Animation Frames At Max Frames
if frame &gt; 4.6 frame_direction = 1;
//Reverse Animation Frames At Minimum Frames
if frame &lt;= 0.2 frame_direction = 0;

if (tile_exists(tile_mask))
    {
        rotate_timer += 1
        if (rotate_timer &gt; 60 and room != rm_editor)
        or (rotate_timer &gt; 20 and room  = rm_editor)
            {
                scr_item_set_mask_offset();
            }
    }

//If The Player Has Destroyed The Gem Flower Tile, Destroy Modified Tile And Self
if (tile_exists(tile_mask) and attacked = 1) and destroyed = 0
    {
        if (room != rm_editor and rotate_angle = 0)
            tile_add(spr_flower_9,0,0,32,36,x-16,y-36,-4);
        var gems_instance = obj_gem;
        if instance_number(obj_gem) &gt; 32 gems_instance = obj_gem_float;
        //Create Crate Particles
        if gems_instance = obj_gem
            {
                //Create Gem Particles If Our Performance Profile Is Set To "Normal"
                if obj_game_logic.performance_profile = 1 and instance_number(obj_destructible_particle) &lt; 16
                    {
                        scr_attack_shield_ext(x+item_x,y+item_y,obj_destructible_particle,0,spr_shield_broken_0,-4  ,1,1,-4)
                        scr_attack_shield_ext(x+item_x,y+item_y,obj_destructible_particle,0,spr_shield_broken_1,-2,1,1,-5.25)
                        scr_attack_shield_ext(x+item_x,y+item_y,obj_destructible_particle,0,spr_shield_broken_2,-1  ,1,1,-5.625)
                        scr_attack_shield_ext(x+item_x,y+item_y,obj_destructible_particle,0,spr_shield_broken_3,0   ,1,1,-7.5)
                        scr_attack_shield_ext(x+item_x,y+item_y,obj_destructible_particle,0,spr_shield_broken_4,2,1,1,-6)
                    }
            }
        //Create Gems
        if instance_number(obj_gem) &lt; 16
            {
                scr_attack_tile(x+item_x,y+item_y,gems_instance,0,spr_gem,0)
                scr_attack_tile(x+item_x,y+item_y,gems_instance,0,spr_gem,-0.5)
                scr_attack_tile(x+item_x,y+item_y,gems_instance,0,spr_gem,-1)
                scr_attack_tile(x+item_x,y+item_y,gems_instance,0,spr_gem,-1.5)
                scr_attack_tile(x+item_x,y+item_y,gems_instance,0,spr_gem,0.5)
                scr_attack_tile(x+item_x,y+item_y,gems_instance,0,spr_gem,1)
                scr_attack_tile(x+item_x,y+item_y,gems_instance,0,spr_gem,1.5)
            }
        if instance_number(obj_gem) &gt;= 16
            scr_attack_tile(x+item_x,y+item_y,obj_red_gem,0,spr_red_gem,0);
        audio_stop_sound(flower_destroy)
        audio_stop_sound(snd_petal_shatter)
        audio_play_sound(flower_destroy,50,false)
        audio_play_sound(snd_petal_shatter,50,false)
        if tile_exists(tile_mask) tile_delete(tile_mask);
        if room != rm_editor instance_destroy();
        else                destroyed = 1;
        //Explode if the game's difficulty is set to level 5
        if obj_game_logic.user_enemy_level &gt;= 4
            {
                var bomb_ID = instance_create(x+item_x,y+item_y,obj_bomb);
                bomb_ID.detonate_for_milla = 1;
                bomb_ID.dont_leave_decorations = 1;
            }
    }
if destroyed &gt; 0
    {
        destroyed += 1;
        if destroyed &gt; 60
            {
                var instance_ID = instance_create(x,y,obj_crystal_gem_flower);
                instance_ID.rotate_angle = rotate_angle;
                instance_destroy();
            }
    }

//Particle Logic

//Advance Animation Frames
part_frame-=0.2
//Float Upward
frame_speed-=1

//Reset Particle Animation and Position
if part_frame &lt;= 0
    {
        part_frame=7    //Reset Animation Frame
        frame_speed=0   //Reset Vertical Offset
        position+=8     //Change Horizontal Position
        //If Horizontal Position Is Past The Tile, Reset Horizontal Offset
        if position&gt;=32 position=0;
    }

} //End of Pause Safety Check
</string>
