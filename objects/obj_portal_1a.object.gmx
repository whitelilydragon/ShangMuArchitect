            <kind>1</kind>
            <string>///Player Interactions
if room = rm_editor
    {
        if (obj_level_editor.use_debug_hud_mode = 1 and o.user_cursor_mode = 0)
        or (o.user_cursor_mode = 1)
            sprite_visible = true;
        else
            sprite_visible = false;
    }
else
    {
        if (o.debug_hud_mode &gt; 0)
            sprite_visible = true;
        else
            sprite_visible = false;
    }

if o.pause = 0
    {
        if transition_allow &lt; 40
            transition_allow += 1;
        prompt_flash += 1
        if prompt_flash &gt; 40
            {
                prompt_flash = 0;
                o.use_warp_point_1A_X = x+16
                o.use_warp_point_1A_Y = y+128
            }
        if obj_game_logic.user_cursor_mode = 0
        and point_in_rectangle(o.universal_player_x,o.universal_player_y,x-32,y,x+sprite_get_width(sprite_index)+32,y+sprite_get_height(sprite_index))
            {
                if transition_active = 0 and o.joy_input[0,3] = 1 and abs(o.hsp) &lt; 0.4
                and transition_allow &gt;= 40 and o.ground = 1
                    {
                        audio_play_sound(pause_exit,50,false)
                        transition_active = 1
                        transition_position = 0
                        transition_allow = 0
                    }
            }
        if transition_active = 1
            {
                transition_position += 31
                if transition_position &gt; 527
                    {
                        o.px = o.use_warp_point_1B_X
                        o.py = o.use_warp_point_1B_Y
                        transition_active = 0
                        transition_position = 0
                    }
            }
        if keyboard_check(vk_anykey) or mouse_check_button(mb_any)
            npc_interface_switch_control_type = 0;
        //Check Controller's Action Buttons
        if gamepad_button_check(o.joy_modern_id,o.joy_up_mod) = 1
            npc_interface_switch_control_type = 1;
    }
</string>
