        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Cube Generator Logic
if o.pause = 0
    {
        //Animations
        frame += 0.5
        if (frame &gt; 1.8) frame = 0;
        //Cube Spawn Timer
        timer += 1
        if (timer &gt; 110) frame = 2;
        if (timer &gt; 120)
            {
                if timer = 121
                    {
                        if instance_number(obj_player_cube_alt) &lt; 5
                            {
                                var cube = instance_create(x,y-30,obj_player_cube_alt);
                                cube.cube_speed = -3
                                cube.cube_gravity = -6
                                audio_play_sound(snd_mud_splat,50,false)
                                audio_play_sound(snd_balloon_pop,50,false)
                            }
                        else
                            timer = 0;
                    }
                if timer &gt; 150
                    timer = 0;
            }
        //Jiggle
        if cube_jiggle_timer &gt; 0
            {
                cube_jiggle_timer -= 0.5
                switch(cube_jiggle_timer)
                    {
                        case 9: {cube_jiggle_scale = 0.5;  break;}
                        case 6: {cube_jiggle_scale = 0.15; break;}
                        case 4: {cube_jiggle_scale = 0.65; break;}
                        case 3: {cube_jiggle_scale = 0.25; break;}
                        case 2: {cube_jiggle_scale = 0.45; break;}
                        case 1: {cube_jiggle_scale =    0; break;}
                    }
            }
        else
            cube_jiggle_timer = 10;
        cube_jiggle = lerp(cube_jiggle,cube_jiggle_scale,0.5)

        //   Particle System   //
        //=====================//
        //Iterate Particle Animation Frames
        skid_animation_frame[0]+=0.40; if skid_animation_frame[0] &gt; 0 {skid_offset_x[0]+=2; skid_offset_y[0]-=0.35;}
        skid_animation_frame[1]+=0.40; if skid_animation_frame[1] &gt; 0 {skid_offset_x[1]+=2; skid_offset_y[1]-=0.35;}
        skid_animation_frame[2]+=0.40; if skid_animation_frame[2] &gt; 0 {skid_offset_x[2]+=2; skid_offset_y[2]-=0.35;}
        
        //Particle Animation Frames 1
        var skid_count = 0;
        
        while(skid_count &lt;= 2)
            {
                if skid_animation_frame[skid_count]&gt;6.8
                    {
                        //Reset skid animation frames and x/y coordinates
                        skid_animation_frame[skid_count] = 1
                        skid_offset_x[skid_count]=x+28
                        skid_offset_y[skid_count]=y-38
                    }
                skid_count += 1
            }
    }
</string>
