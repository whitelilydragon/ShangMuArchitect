event_inherited(); 

//Animate Flounders Outside the Fish Bowl
var count = 0;
while(count &lt;= 3)
    {
        //Cry
        if actor_flounder_state[count] = 0
            {
                actor_flounder_frame[count] += 0.25
                if actor_flounder_frame[count] &gt; 22.8
                    actor_flounder_frame[count] = 21;
            }
        //Fly Up
        if actor_flounder_state[count] = 1
            {
                actor_flounder_frame[count] += 0.25
                if actor_flounder_frame[count] &gt; 22.8
                    actor_flounder_frame[count] = 21;
                if instance_exists(smart_pool_id)
                    {
                        actor_flounder_x[count] = lerp(actor_flounder_x[count],smart_pool_id.x+64,0.05)
                        actor_flounder_y[count] -= 6
                        if actor_flounder_y[count] &lt; smart_pool_id.y
                            actor_flounder_state[count] = 2;
                    }
            }
        //Fall Down
        if actor_flounder_state[count] = 2
            {
                if instance_exists(smart_pool_id)
                    {
                        actor_flounder_y[count] = lerp(actor_flounder_y[count],smart_pool_id.y+64,0.05)
                        if actor_flounder_splash[count] = 0 and actor_flounder_y[count] &gt;= smart_pool_id.y
                            {
                                instance_create(actor_flounder_x[count],smart_pool_id.y,obj_cutscene_13_splash)
                                actor_flounder_splash[count] = 1
                            }
                        //Swim upon sinking into the water
                        if actor_flounder_y[count] &gt;= smart_pool_id.y+56
                            {
                                actor_flounder_frame[count] = 0
                                actor_flounder_state[count] = 3
                            }
                    }
            }
        //Swim
        if actor_flounder_state[count] = 3
            {
                if instance_exists(smart_pool_id)
                    {
                        if actor_flounder_x[count] &gt; smart_pool_id.x + 112
                            actor_flounder_direction[count] = -1;
                        if actor_flounder_x[count] &lt; smart_pool_id.x + 16
                            actor_flounder_direction[count] = 1

                        actor_flounder_x[count] += 1 * actor_flounder_direction[count]

                        actor_flounder_frame[count] += 0.25
                        if actor_flounder_frame[count] &gt; 17.8
                            actor_flounder_frame[count] = 0;
                    }
            }
        //Swim Towards Milla
        if actor_flounder_state[count] = 4
            {
                actor_flounder_frame[count] += 0.25

                if actor_flounder_frame[count] &lt; 24
                    actor_flounder_frame[count] = 24;
                if actor_flounder_frame[count] &gt; 25.8
                    actor_flounder_frame[count] = 24;

                actor_flounder_direction[count] = 1

                actor_flounder_x[count] = lerp(actor_flounder_x[count],actor_milla_x-32,0.05)
                actor_flounder_y[count] = lerp(actor_flounder_y[count],actor_plane_y-48,0.025)
            }
        //Swim Up and Down
        if actor_flounder_state[count] = 5
        or actor_flounder_state[count] = 6
            {
                actor_flounder_frame[count] += 0.25

                if actor_flounder_frame[count] &lt; 26
                    actor_flounder_frame[count] = 26;
                if actor_flounder_frame[count] &gt; 27.8
                    actor_flounder_frame[count] = 26;

                if actor_flounder_state[count] = 5
                    {
                        actor_flounder_y[count] = lerp(actor_flounder_y[count],actor_plane_y-64,0.075)
                        if actor_flounder_y[count] &lt;= actor_plane_y-60
                            actor_flounder_state[count] = 6;
                    }
                if actor_flounder_state[count] = 6
                    {
                        actor_flounder_y[count] = lerp(actor_flounder_y[count],actor_plane_y-32,0.075)
                        if actor_flounder_y[count] &gt;= actor_plane_y-38
                            actor_flounder_state[count] = 5;
                    }
            }
        //Fly Above the Bowl and Bounce Forward
        if actor_flounder_state[count] &gt;= 7
            {
                //Animations
                actor_flounder_frame[count] += 0.25
                if actor_flounder_frame[count] &gt; 17.8
                    actor_flounder_frame[count] = 0;

                //Jump
                if actor_flounder_state[count] = 7
                    {
                        actor_flounder_gravity[count] -= 8
                        actor_flounder_state[count] = 8
                    }
                if actor_flounder_state[count] = 8
                    actor_flounder_x[count] = lerp(actor_flounder_x[count],actor_milla_x+96,0.075);
                //Gravity Speed and Cap
                actor_flounder_gravity[count] += 0.24
                if actor_flounder_gravity[count] &gt; 8
                    actor_flounder_gravity[count] = 0;
                //Apply Speed
                actor_flounder_y[count] += actor_flounder_gravity[count]
                //Clip Speed and Height to Actor Plane
                if actor_flounder_y[count] &gt; actor_plane_y-10
                    {
                        actor_flounder_gravity[count] = -4
                        actor_flounder_y[count] = actor_plane_y-8
                    }
            }
        count += 1
    }
