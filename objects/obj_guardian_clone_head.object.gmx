        if point_distance(o.px,o.py,x,y) &lt; 48
        and o.hurt[0] = 0 and o.hurt[1] = 0
        and o.shield_invuln = 32
        and not (o.shield_active = 1 and o.shield=spr_shield_earth)
            scr_enemy_deal_damage(0.4,1,2);
    }
