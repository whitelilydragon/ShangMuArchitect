            <kind>1</kind>
            <string>if obj_game_logic.pause = 0
    {
        switch(state)
            {
                //Idle
                case 0:
                    {
                        frame += 0.05
                        if frame &gt; 5.8
                            {
                                frame = 0;
                                xdir = sign(random_range(-10,10))
                            }
                        if point_distance(o.px,o.py,x,y) &lt; 64
                        and obj_game_logic.user_cursor_mode = 0
                            {
                                frame = 6
                                state = 1
                                audio_play_sound(snd_bird_flap,50,false)
                            }
                        break;
                    }
                case 1:
                    {
                        frame += 0.15
                        if frame &gt; 9.8
                            frame = 6;
                        x += 4*xdir
                        y -= 2;
                        //Return to starting position if we are moused over during editor mode
                        if room = rm_editor
                            {
                                var parent = obj_level_editor
                                if parent.use_write_delay &gt; 0
                                or ((point_in_rectangle(parent.use_cursor_x,parent.use_cursor_y,start_x-width_x,start_y-height_y,start_x+width_x,start_y)
                                or point_in_rectangle(parent.use_cursor_x,parent.use_cursor_y,x-width_x,y-height_y,x+width_x,y))
                                and parent.user_cursor_mode = 0 and parent.use_tile_menu = 0)
                                //Or return to starting position if we've left the boundaries of the camera
                                or !point_in_rectangle(x,y,view_xview[view_port]-64,view_yview[view_port]-64,view_xview[view_port]+view_wview[view_port]+64,view_yview[view_port]+view_hview[view_port]+64)
                                    {
                                        x = start_x
                                        y = start_y
                                        state = 0
                                        frame = 0
                                    }
                            }
                        //If we are not in editor mode, disappear after a certain distance,
                        //or if too many instances of birds are fluttering at once
                        else
                            {
                                if y &lt; 0 or x &gt; room_width or x &lt; 0
                                or instance_number(obj_bird) &gt; 64
                                    {
                                        instance_destroy();
                                    }
                            }
                        break;
                    }
            }
    }
</string>
