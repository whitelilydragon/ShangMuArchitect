        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Rotate Spike Tiles Around Main Spike Tile
if obj_game_logic.pause = 0
    {
        //Set Spike Tiles in Orbit Around Origin Point
        mu_spike_0_x = x
        mu_spike_0_y = y
        mu_spike_1_x = x+lengthdir_x(32,direction)
        mu_spike_1_y = y+lengthdir_y(32,direction)
        mu_spike_2_x = x+lengthdir_x(64,direction)
        mu_spike_2_y = y+lengthdir_y(64,direction)
        mu_spike_3_x = x+lengthdir_x(96,direction)
        mu_spike_3_y = y+lengthdir_y(96,direction)
        mu_spike_4_x = x+lengthdir_x(128,direction)
        mu_spike_4_y = y+lengthdir_y(128,direction)
        //Search for positions to deal damage
        if point_distance(obj_game_logic.universal_player_x,obj_game_logic.universal_player_y,mu_spike_0_x+16,mu_spike_0_y+16) &lt; obj_game_logic.player_hitbox_radius/2
        or point_distance(obj_game_logic.universal_player_x,obj_game_logic.universal_player_y,mu_spike_1_x+16,mu_spike_1_y+16) &lt; obj_game_logic.player_hitbox_radius/2
        or point_distance(obj_game_logic.universal_player_x,obj_game_logic.universal_player_y,mu_spike_2_x+16,mu_spike_2_y+16) &lt; obj_game_logic.player_hitbox_radius/2
        or point_distance(obj_game_logic.universal_player_x,obj_game_logic.universal_player_y,mu_spike_3_x+16,mu_spike_3_y+16) &lt; obj_game_logic.player_hitbox_radius/2
        or point_distance(obj_game_logic.universal_player_x,obj_game_logic.universal_player_y,mu_spike_4_x+16,mu_spike_4_y+16) &lt; obj_game_logic.player_hitbox_radius/2
            {
                if (obj_game_logic.attack[0] = 1 or obj_game_logic.attack[3] = 1)
                and obj_game_logic.pause_logic = 0
                
                    {
                        audio_play_sound(character_parry,50,false)
                        obj_game_logic.pause_logic = 5
                    }       
                //Do not harm the player if they have a spike shield
                if  not (obj_game_logic.shield_active = 1 and obj_game_logic.shield=spr_shield_metal)
                and not (obj_game_logic.attack[0] = 1 or obj_game_logic.attack[1] &gt; 0)
                and (obj_game_logic.invuln_hud_acti = 0) and (obj_game_logic.hurt[0] = 0
                and obj_game_logic.shield_invuln = 32 and obj_game_logic.pause_logic = 0)
                    scr_enemy_deal_damage(0.4,1,2);
            }
        //Modify Angle of Rotation
        direction -= 2
        //Reset To Our Starting Position, If The Game Is Saving
        if room = rm_editor
            {
                var parent = obj_level_editor
                if parent.use_write_delay &gt; 0
                    {
                        x = x_start
                        y = y_start
                    }
                else
                    {
                        x = x_start_1
                        y = y_start_1
                    }
            }
    }
</string>
