                        audio_play_sound(snd_boost_pad,50,false)
                        audio_play_sound(snd_tank_moving,50,false)
                        audio_play_sound(savvy_scream,50,false)
                    }
            }

        if cutscene_command = 1
        and obj_game_logic.user_cursor_mode = 0
