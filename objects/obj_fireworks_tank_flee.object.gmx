                if room != rm_campaign
                    instance_end_timer += 1;
                if instance_end_timer &gt;= 60
                    {
                        if room = rm_editor
                            {
                                instance_create(start_x,start_y,obj_fireworks_tank_flee)
                                instance_destroy();
                            }
                        else
                            instance_destroy();
                    }
                cutscene_timer = 0
                tank_direction = 1
                heart = heartmax/3
                cutscene_speed += 0.50
                if cutscene_speed &gt; 16
                    cutscene_speed = 16
                x += cutscene_speed*cutscene_direction
                hsp = 2
                tile_angle = 0
                scr_set_screen_shake(3,1)
            }
