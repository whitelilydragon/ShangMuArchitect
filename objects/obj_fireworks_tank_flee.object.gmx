                if npc_dialog_iterate &gt;= string_length(npc_dialog_string)
                    {
                        savvy_dialogue_timer += 1
                        if savvy_dialogue_timer &gt; 60
                            {
                                savvy_dialogue_enabled = 0;
                                audio_stop_sound(snd_tank_moving)
                                audio_stop_sound(snd_tank_idle)
                                instance_destroy(self,false)
                            }
                        npc_dialog_sound = 0
                    }
                //Play text scrolling sound effect
                if npc_dialog_sound &gt;= 40
                    {
                        audio_play_sound(npc_talk_voice,50,false)
                        npc_dialog_sound = 0
                    }
            }
        //Run if the player sees us
        if  obj_game_logic.px &gt; x-128
        and obj_game_logic.py &gt; y-128
        and obj_game_logic.py &lt; y+16
