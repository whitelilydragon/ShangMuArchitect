        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Pause Safety Check
if obj_game_logic.pause = 0
{
if (tile_exists(tile_mask))
    {
        rotate_timer += 1
        if (rotate_timer &gt; 60 and room != rm_editor)
        or (rotate_timer &gt; 20 and room  = rm_editor)
            scr_item_set_mask_offset();
    }

//Animation System
frame += frame_speed
if (frame &gt; frame_end) frame = 0;
if shield_sprite = spr_shield_rock
    {
        shield_rotate_speed = lerp(shield_rotate_speed,shield_rotate_target,0.25);
        shield_rotate -= shield_rotate_speed;
        shield_rotate_timer += 1
        if shield_rotate_timer = 64
            shield_rotate_target = 16;
        if shield_rotate_timer &gt;= 128
            {
                shield_rotate_target = 2
                shield_rotate_timer = 0
            }
    }
//If The Player Has Destroyed The Crate, Destroy Modified Tile And Self
if (attacked = 1 and destroyed = 0)
    {
        if (room != rm_editor and rotate_angle = 0)
            tile_add(spr_flower_9,0,0,32,36,x-16,y-36,-4);
        //Create Crate Particles
        if instance_number(obj_collectable) &lt; 16
            {
                //Create Gem Particles If Our Performance Profile Is Set To "Normal"
                if obj_game_logic.performance_profile = 1 and instance_number(obj_destructible_particle) &lt; 16
                    {
                        scr_attack_shield_ext(x+item_x,y+item_y,obj_destructible_particle,0,spr_shield_broken_1,-4  ,1,1,-5.25)
                        scr_attack_shield_ext(x+item_x,y+item_y,obj_destructible_particle,0,spr_shield_broken_4,-2,1,1,-6)
                        scr_attack_shield_ext(x+item_x,y+item_y,obj_destructible_particle,0,spr_shield_broken_3,-1  ,1,1,-7.5)
                        scr_attack_shield_ext(x+item_x,y+item_y,obj_destructible_particle,0,spr_shield_broken_0,0   ,1,1,-4)
                        scr_attack_shield_ext(x+item_x,y+item_y,obj_destructible_particle,0,spr_shield_broken_2,2,1,1,-5.625)
                    }
            }
        obj_game_logic.shield_flash_trigger = 1
        obj_game_logic.shield_flash_frame   = 8 
        obj_game_logic.shield_flash_x       = x+item_x
        obj_game_logic.shield_flash_y       = y+item_y
        scr_set_shield(shield_sprite)
        audio_stop_sound(shield_break)
        audio_stop_sound(snd_petal_shatter)
        audio_play_sound(shield_break,50,false)
        audio_play_sound(snd_petal_shatter,50,false)
        if tile_exists(tile_mask) tile_delete(tile_mask);
        if room = rm_editor destroyed = 1;
        else                instance_destroy();
        //Explode if the game's difficulty is set to level 5
        if obj_game_logic.user_enemy_level &gt;= 4
            {
                var bomb_ID = instance_create(x+item_x,y+item_y,obj_cube_rogue_bomb);
                bomb_ID.detonate_for_milla = 1;
                bomb_ID.dont_leave_decorations = 1;
            }
    }
if destroyed &gt; 0
    {
        destroyed += 1;
        if destroyed &gt; 60
            {
                instance_copy(true);
                instance_destroy();
            }
    }
} //End of Pause Safety Check
</string>
