        <arguments>
          <argument>
            <kind>1</kind>
            <string>var o = obj_game_logic;

if o.pause = 0
    {
        //Check For Collisions With Spring's Surface
        if point_in_rectangle(o.px,o.py,x+spring_left,y+spring_top,x+spring_right,y+spring_bottom)
        and o.spring_rotate != spring_rotated
        and o.user_cursor_mode = 0
            {
                scr_set_spring_launch(spring_speed,spring_timer,spring_rotated)
                if spring_sound_id = 0
                    {
                        audio_stop_sound(snd_enemy_sword)
                        audio_stop_sound(character_spring)
                        audio_play_sound(snd_enemy_sword,50,false)
                        audio_play_sound(character_spring,50,false)
                        //Do not harm the player if they have a metal shield
                        if not (o.shield_active = 1 and o.shield=spr_shield_metal)
                            {
                                if o.hurt[0] = 0
                                and o.shield_invuln = 32
                                    scr_enemy_deal_damage(0.4,1,2);
                            }
                        spring_sound_id = 4
                    }
                spring_triggered = 1
                frame = 1
                spring_particle_frame = 0
                if spring_rotated = 0
                    {
                        if o.px &lt; x+16 o.px = x+24;
                        o.hsp = spring_speed;
                        player_apply_speed = 2
                    }
                if spring_rotated = 180
                    {
                        if o.px &gt; x+16 o.px = x+16;
                        o.hsp = -spring_speed;
                        player_apply_speed = -2
                    }
                if spring_rotated = 270 o.py = y+48;
            }
        //Animation System
        if spring_triggered = 1
            {
                spring_particle_frame += 0.48
                if spring_particle_frame &gt;= 4
                    frame = 0;
                if spring_particle_frame &gt;= 7.8
                    {
                        spring_particle_frame = 0
                        spring_triggered = 0
                        spring_sound_id = 0
                        frame = 0
                    }
            }
    }
</string>
