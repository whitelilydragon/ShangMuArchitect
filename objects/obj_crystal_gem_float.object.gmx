            <string>//Pause Safety Check
if obj_game_logic.pause = 0
{
//Advance Frames
if frame_direction = 0 frame+=0.15;
else                   frame-=0.15;
//Reverse Animation Frames At Max Frames
if frame &gt; 4.6 frame_direction = 1;
//Reverse Animation Frames At Minimum Frames
if frame &lt;= 0.2 frame_direction = 0;

//If The Player Has Destroyed The Gem Flower Tile, Destroy Modified Tile And Self
if (attacked = 1) and destroyed = 0
    {
        var gems_instance = obj_gem;
        //Create Crate Particles
        if gems_instance = obj_gem
            {
                //Create Gem Particles If Our Performance Profile Is Set To "Normal"
                if obj_game_logic.performance_profile = 1 and instance_number(obj_destructible_particle) &lt; 16
                    {
                        scr_attack_shield_ext(x,y-20,obj_destructible_particle,0,spr_shield_broken_0,-4  ,1,1,-4)
                        scr_attack_shield_ext(x,y-20,obj_destructible_particle,0,spr_shield_broken_1,-2,1,1,-5.25)
                        scr_attack_shield_ext(x,y-20,obj_destructible_particle,0,spr_shield_broken_2,-1  ,1,1,-5.625)
                        scr_attack_shield_ext(x,y-20,obj_destructible_particle,0,spr_shield_broken_3,0   ,1,1,-7.5)
                        scr_attack_shield_ext(x,y-20,obj_destructible_particle,0,spr_shield_broken_4,2,1,1,-6)
                    }
            }
        //Create Gems
        if instance_number(obj_crystal_gem_float_gem) &lt; 32
            {
                var gem_count = 0;
                var gem_rotate = 0;
                while(gem_count &lt;= 16)
                    {
                        var gem_count_id = instance_create(x,y-20,obj_crystal_gem_float_gem);
                        gem_rotate += 22.5
                        gem_count += 1

                        gem_count_id.direction = gem_rotate
                        gem_count_id.speed = 4
                        gem_count_id.direction_reserve = gem_rotate
                        gem_count_id.speed_reserve = 4
                    }
            }
        if instance_number(obj_crystal_gem_float_gem) &gt;= 32
            {
                var gem_count = 0;
                var gem_rotate = 0;
                while(gem_count &lt;= 4)
                    {
                        var gem_count_id = instance_create(x,y-20,obj_crystal_gem_float_gem);
                        gem_rotate += 90
                        gem_count += 1

                        gem_count_id.direction = gem_rotate
                        gem_count_id.speed = 4
                        gem_count_id.direction_reserve = gem_rotate
                        gem_count_id.speed_reserve = 4
                    }
            }
        audio_stop_sound(flower_destroy)
        audio_stop_sound(snd_petal_shatter)
        audio_play_sound(flower_destroy,50,false)
        audio_play_sound(snd_petal_shatter,50,false)
        if room != rm_editor destroyed = -1;
        else                 destroyed = 1;
        //Explode if the game's difficulty is set to level 5
        if obj_game_logic.user_enemy_level &gt;= 4
            {
                var bomb_ID = instance_create(x,y-20,obj_cube_rogue_bomb);
                bomb_ID.detonate_for_milla = 1;
                bomb_ID.dont_leave_decorations = 1;
            }
    }
if destroyed &gt; 0
    {
        destroyed += 1;
        if destroyed &gt; 60
            {
                var instance_ID = instance_create(x,y,obj_crystal_gem_float);
                instance_destroy();
            }
    }

//Particle Logic

//Advance Animation Frames
part_frame-=0.2
//Float Upward
frame_speed-=1

//Reset Particle Animation and Position
if part_frame &lt;= 0
    {
        part_frame=7    //Reset Animation Frame
        frame_speed=0   //Reset Vertical Offset
        position+=8     //Change Horizontal Position
        //If Horizontal Position Is Past The Tile, Reset Horizontal Offset
        if position&gt;=32 position=0;
    }

} //End of Pause Safety Check
</string>
