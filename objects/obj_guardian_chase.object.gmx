            <kind>1</kind>
            <string>///Runaway Guardian Logic
if o.pause = 0
    {
        if point_in_rectangle(o.px,o.py,x,y-128,x+32,y+128)
        and (o.user_cursor_mode = 0 and timer = 0)
            timer = 1;
        if timer &gt; 0
            {
                timer += 1
                if timer = 16
                    {
                        audio_stop_sound(snd_dragon_roar)
                        audio_play_sound(snd_dragon_roar,50,false)
                        audio_play_sound(snd_dragon_roar,50,false)
                        audio_play_sound(snd_dragon_roar,50,false)
                    }
                //Horizontal Movement
                sprite_x += 6
                //Vertical Movement
                sprite_accel += 0.05
                if sprite_accel &gt; 10
                    sprite_accel = 10;
                sprite_y -= sprite_accel
                //Exit Conditions
                if sprite_x &gt; x + 480
                    {
                        scr_set_screen_shake(6,3)
                        audio_play_sound(snd_explosion,50,false)
                        if room = rm_editor
                            {
                                timer = 0
                                sprite_x = x-256
                                sprite_y = y+128
                                sprite_speed = 0
                                sprite_accel = 4
                            }
                        else
                            instance_destroy();
                    }
            }
    }
</string>
