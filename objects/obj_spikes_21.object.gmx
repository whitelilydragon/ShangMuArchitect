        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Spike Logic
if o.pause = 0
    {
        if (tile_exists(tile_mask))
            {
                rotate_timer += 1
                if (rotate_timer &gt; 60 and room != rm_editor)
                or (rotate_timer &gt; 20 and room  = rm_editor)
                    {
                        scr_item_set_mask_offset();
                    }
            }
        //Damage the player if they are standing on us
        if point_in_rectangle(o.px,o.py,x-16,y-36,x+16,y-24)
            {
                //Do not harm the player if they have a spike shield
                if  not (o.shield_active = 1 and o.shield=spr_shield_metal)
                and (o.invuln_hud_acti = 0) and (o.hurt[0] = 0
                and o.shield_invuln = 32 and o.pause_logic = 0)
                    {
                        scr_enemy_deal_damage(0.4,1,2)
                        audio_play_sound(snd_petal_shatter,50,false)
                    }
            }
        if frame_enable = 1
            {
                frame += 0.5
                if frame &gt; 5.8
                    {
                        frame = 0
                        frame_enable = 0
                    }
            }
        if frame_enable &lt;= 0
            {
                frame_enable -= 1
                if frame_enable &lt;= -30
                    frame_enable = 1;
            }
        //Destroy ourselves if we're attacked by the player
        if attacked = 1
            {
                if timer = 0
                    {
                        if tile_exists(tile_mask)
                            tile_delete(tile_mask);
                        audio_play_sound(snd_rock_destroyed,50,false)
                        audio_play_sound(snd_petal_shatter,50,false)
                        //Create Gem Particles If Our Performance Profile Is Set To "Normal"
                        if obj_game_logic.performance_profile = 1 and instance_number(obj_destructible_particle) &lt; 16
                            {
                                scr_attack_shield_ext(x,y-16,obj_destructible_particle,0,spr_spikes_21_particle,-4  ,1,1,-4)
                                scr_attack_shield_ext(x,y-16,obj_destructible_particle,0,spr_spikes_21_particle,-2,1,1,-5.25)
                                scr_attack_shield_ext(x,y-16,obj_destructible_particle,0,spr_spikes_21_particle,1  ,1,1,-5.625)
                                scr_attack_shield_ext(x,y-16,obj_destructible_particle,0,spr_spikes_21_particle,4   ,1,1,-7.5)
                            }
                    }
                if room = rm_editor
                    {
                        timer += 1
                        if timer &gt;= 60
                            {
                                instance_create(x,y,obj_spikes_21);
                                instance_destroy();
                            }
                    }
                else
                    instance_destroy();
            }
    }
</string>
