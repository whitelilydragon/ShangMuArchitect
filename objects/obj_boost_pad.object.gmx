        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>var o = obj_game_logic;

if o.pause = 0
    {
        timer += 1
        if timer &gt; 8 timer = 8;
        //Check For Collisions With Spring's Surface
        if point_in_rectangle(o.px,o.py,x-24,y-32,x+24,y)
        and o.user_cursor_mode = 0
        and timer = 8
            {
                timer = 0
                if abs(o.hsp) &lt; 12
                    o.hsp = 12*o.xdir;
                audio_play_sound(snd_boost_pad,50,false)
            }
    }

if room = rm_editor
    {
        var parent = obj_level_editor;
        //Display FOF Overlay If Hovered Over
        if (point_in_rectangle(parent.use_cursor_x,parent.use_cursor_y,x-24,y-32,x+24,y)
        and parent.use_tile_menu = 0 and parent.side_bar_allow = 0)
            fof_overlay_enable = 1;

        if fof_overlay_enable = 1
            {
                if device_mouse_x_to_gui(0) &lt; parent.user_editor_hud_offset-6
                    parent.user_place_allow = 1;
                if !point_in_rectangle(parent.use_cursor_x,parent.use_cursor_y,x-24,y-54,x+24,y)
                or device_mouse_x_to_gui(0) &gt;= parent.user_editor_hud_offset-6
                    {
                        fof_overlay_enable = 0
                        parent.user_place_allow = 0
                    }
                if (mouse_check_button_pressed(parent.joy_mouse_button_left) or parent.use_put_here = 1)
                and parent.use_cursor_y &lt; y-8
                    {
                        angle += 90
                        if angle &gt; 360
                            angle = 90;
                        use_confirm = 1;
                        //Play menu sound
                        audio_play_sound(snd_turn_page,50,false)
                    }
                if (mouse_check_button_pressed(parent.joy_mouse_button_right) or parent.use_delete = 1)
                and parent.use_cursor_y &lt; y-8
                    {
                        angle -= 90
                        if angle &lt; 0
                            angle = 270;
                        use_confirm = 1;
                        //Play menu sound
                        audio_play_sound(snd_turn_page,50,false)
                    }
                //Change boost pad object based on angle
                switch(angle)
                    {
                        case 0:   {instance_change(obj_boost_pad,true);     break;}
                        case 90:  {instance_change(obj_boost_pad_90,true);  break;}
                        case 180: {instance_change(obj_boost_pad_180,true); break;}
                        case 270: {instance_change(obj_boost_pad_270,true); break;}
                        case 360: {instance_change(obj_boost_pad,true);     break;}
                    }
            }
    }
</string>
          </argument>
