            <string>///Animate background
event_inherited()
if obj_game_logic.pause = 0 
    {
        if  view_wview[view_port] &gt; obj_game_logic.use_zoom_x_view-2
        and view_wview[view_port] &lt; obj_game_logic.use_zoom_x_view+2
            bubbles_paralax_y += 2*obj_game_logic.use_zoom_multiplier;
        else
            bubbles_paralax_y += 0.010;
        if bubbles_paralax_y &gt;= 360*obj_game_logic.use_zoom_multiplier
            bubbles_paralax_y = 0;
        bubbles_frames += 0.25
        if bubbles_frames &gt; 1.8
            bubbles_frames = 0;

        cube_jiggle_timer -= 0.25
        switch(cube_jiggle_timer)
            {
                case 9: {cube_jiggle_scale = 0.5; break;}
                case 6: {cube_jiggle_scale = 0.15; break;}
                case 4: {cube_jiggle_scale = 0.55; break;}
                case 3: {cube_jiggle_scale = 0.15; break;}
                case 2: {cube_jiggle_scale = 0.55; break;}
                case 1: {cube_jiggle_scale = 0; break;}
            }
        if cube_jiggle_timer &lt; 0 cube_jiggle_timer = 10
        cube_jiggle = lerp(cube_jiggle,cube_jiggle_scale,0.25)
    }
</string>
