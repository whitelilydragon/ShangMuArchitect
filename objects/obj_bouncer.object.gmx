    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>event_inherited()

if room = rm_editor
    {
        editor_mode_timer += 1

        if editor_mode_timer = 30
            {
                if o.user_cursor_mode = 1
                    mask_index = spr_bouncer_thumbnail;
                else
                    {
                        if xdir = -1
                            mask_index = spr_bouncer_mask_left;
                        if xdir = 1
                            mask_index = spr_bouncer_mask;
                    }
            }
        //Prevent Certain Mod Chips From Activating
        //if instance_number(obj_bouncer) &gt; 1
        //and object_index != obj_bouncer_flee
        //    {
        //        scr_set_message("There can only be one Bouncer per stage!")
        //        instance_destroy();
        //    }
        if item = spr_mod_chip_extra_large
            {
                item = 0
                scr_set_message("Bouncer is big enough to take you on!")
            }
        if point_distance(o.px,o.py,x,y) &gt; 1024
            phase = 0;
    }

if o.pause = 0 and heart &gt;= 0
    {
        spring_speed = 0
        image_angle = body_angle+angle_offset
        lantern_frame += 0.20
        if lantern_frame &gt; 2.8
            lantern_frame = 0;
        //Parry Box
        if instance_exists(parry_box)
            {
                parry_box.x = body_x
                parry_box.y = body_y
                parry_box.image_xscale = xdir
            }
        //Splash Effects
        can_splash_wave_frame += 0.75
        if can_splash_wave_frame &gt; 5.8
            can_splash_wave_frame = 0;

        //Animate Lantern Segments
        if target_frame &gt; 0
            {
                target_frame += 0.15
                if target_frame &gt; 4.8
                    target_frame = 0;
            }
        //Bobble Lantern When Attacked
        if alpha &gt; 0 and can_explode_lantern = 0
            {
                target_timer = 31
                target_speed = 8*o.xdir
                can_explode_lantern = 1
                audio_play_sound(snd_enemy_dead,50,false)
            }
        if target_timer &gt; 0
            {
                target_timer -= 1
                switch(target_timer)
                    {
                        case 0: {target_lerp = 0; can_explode_lantern = 0; break;}
                        case 1: {target_lerp = 0; can_explode_lantern = 0; break;}
                        case 20:
                            {
                                if can_explode_lantern = 1
                                    {
                                        var temp_bomb_id = instance_create(target_x,target_y,obj_bouncer_bomb);
                                        temp_bomb_id.detonate_do_not_harm_player = 1
                                        temp_bomb_id.detonate = 1
                                        temp_bomb_id.detonate_delay = 1
                                        temp_bomb_id.frame_decay = -8
                                    }
                                break;
                            }
                        case 15: {target_lerp = -target_speed; break;}
                        case 30: {target_lerp = target_speed; break;}
                    }
            }
        target_offset = lerp(target_offset,target_lerp,0.25)
        //Shoot Bubbles
        if can_open_mouth = 1
            {
                //Shoot Bubbles
                can_open_mouth_timer += 1

                if can_open_mouth_timer = 24
                    {
                        obj_id = instance_create(x,y-4,obj_bouncer_bomb)
                        obj_id.float_speed = 20*xdir
                        audio_play_sound(snd_bouncy_cube,50,false)
                    }
                if can_open_mouth_timer &gt;= 48
                    {
                        can_open_mouth = 0
                        can_open_mouth_timer = 0
                    }
            }
        if boss_phase_2 = 0 and heart &lt; heartmax/2
            {
                audio_play_sound(snd_boss_phase_transition,50,false);
                phase_timer = 1;
                boss_phase_2 = 1;
            }
        //Turn Around
        if turn = 1
            {
                if xdir = -1
                    {
                        body_angle = 180
                        lantern_angle = 69.5
                        leg_attach_point_0 = 47.3
                        leg_attach_point_1 = 52.4
                        lantern_mount_angle[0] = -0
                        lantern_mount_angle[1] = -10.62
                        lantern_mount_angle[2] = -7.13
                        lantern_mount_angle[3] = -1.19
                        lantern_mount_angle[4] = 4.54
                        lantern_mount_angle[5] = 11.02
                        lantern_mount_angle[6] = 16.7
                        lantern_mount_angle[7] = 24
                        mask_index = spr_bouncer_mask_left
                    }
                if xdir = 1
                    {
                        body_angle = 0
                        lantern_angle = 0
                        leg_attach_point_0 = 52.4
                        leg_attach_point_1 = 47.3
                        lantern_mount_angle[0] = 0
                        lantern_mount_angle[1] = 10.62
                        lantern_mount_angle[2] = 7.13
                        lantern_mount_angle[3] = 1.19
                        lantern_mount_angle[4] = -4.54
                        lantern_mount_angle[5] = -11.02
                        lantern_mount_angle[6] = -16.7
                        lantern_mount_angle[7] = -24
                        mask_index = spr_bouncer_mask
                    }
                turn = 0
                target_offset = 0
            }

        if (phase &gt; 0)
            {
                //Attack Scanning//
                //---------------//

                //Hurt The Player If They've Been Punched By The Boxing Glove
                if (o.hurt[0] = 0 and o.hurt[1] = 0
                and o.shield_invuln = 32
                and (point_in_circle(hitbox_x,hitbox_y,o.universal_player_x,o.universal_player_y,64))
                or point_in_circle(x,y,o.universal_player_x,o.universal_player_y,32))
                    {
                        scr_enemy_deal_damage(0.4,0.4,2);
                        o.hsp = 8*sign(o.hsp)
                        target_frame = 0.1
                        if play_sound = 1
                            audio_play_sound(snd_enemy_sword,50,false);
                    }
                //Mega Items
                var instance_ID = collision_circle(hitbox_x,hitbox_y,64,obj_mega_item_parent,false,true);
                if instance_exists(instance_ID)
                    {
                        instance_ID.attacked = 2
                    }
                //Items
                var instance_ID = collision_circle(hitbox_x,hitbox_y,64,obj_destructible,false,true);
                if instance_exists(instance_ID)
                    {
                        if instance_ID.object_index != obj_bouncer_bomb
                        and instance_ID.object_index != obj_bouncer_parry_box
                            instance_ID.attacked = 1;
                    }
                //Attack other enemies within range of the boxing glove
                instance_ID = collision_circle(hitbox_x,hitbox_y,64,obj_enemy_parent,false,true);
                if instance_exists(instance_ID)
                    {
                        if instance_ID.attacked = 0 and instance_ID.alpha = 0
                            {
                                instance_ID.attacked = 1
                                instance_ID.attacked_damage = 2
                            }
                    }
            }

        //Movement System
        switch(phase)
            {
                //Idle Phase
                case 0:
                    {
                        timer += 1
                        body_timer+= 1
                        switch(body_timer)
                            {
                                case 0:  {body_target = 0;  body_wiggle = 4; break;}
                                case 15: {body_target = -8; body_wiggle = 0; break;}
                                case 30: {body_target = 0;  body_wiggle = 8; break;}
                                case 45: {body_target = 32; body_wiggle = 4; break;}
                                case 60: {body_target = 16; body_wiggle = 0; body_timer = 0; break;}
                            }
                        if (ground = 1 or gravity_slope = 1)
                            {
                                body_x = lerp(body_x,x+body_target,0.1)
                                body_y = lerp(body_y,y-82-body_wiggle,0.1)
                                foot_angle = tile_angle
                                ground_timer += 1
                                if tile_angle &gt; 45 and tile_angle &lt; 180
                                or tile_angle &gt; 225 and tile_angle &lt; 315
                                    turn = 1;
                                if boss_phase_2 = 1
                                    {
                                        if timer &gt;= 90 and can_open_mouth_timer = 0
                                            {
                                                can_open_mouth = 1
                                                can_open_mouth_timer = 0
                                                can_open_mouth_water_timer = 0
                                            }
                                    }
                            }
                        else
                            {
                                var body_mod = -72;
                                if can_swim = 1
                                    body_mod = -72+can_hover_timer;
                                body_x = lerp(body_x,x+8,0.1)
                                body_y = lerp(body_y,y+body_mod,0.1)
                                foot_angle = body_angle
                                ground_timer = 0
                                if y &gt; o.water_height-8
                                    can_swim = 1;
                                else
                                    can_swim = 0;
                                if can_swim = 1
                                    {
                                        if can_splash_x_0 &gt;= 0
                                            can_splash_frame_0 += 0.5;
                                        if can_splash_frame_0 &gt; 8.8
                                            {
                                                can_splash_x_0 = -64;
                                                can_splash_frame_0 = 0;
                                            }

                                        if can_splash_x_1 &gt;= 0
                                            can_splash_frame_1 += 0.5;
                                        if can_splash_frame_1 &gt; 8.8
                                            {
                                                can_splash_frame_1 = 0
                                                can_splash_x_1 = -64;
                                            }
                                        can_splash_timer += 1
                                        if can_splash_timer &gt;= 96
                                            can_splash_timer = 0;

                                        can_open_mouth_water_timer += 1
                                        if can_open_mouth_water_timer &gt;= 120
                                        and can_open_mouth_timer = 0
                                            {
                                                can_open_mouth = 1
                                                can_open_mouth_timer = 0
                                                can_open_mouth_water_timer = 0
                                            }

                                        //Sound Effects
                                        motor_sound_timer += 1
                                        if motor_sound_timer = 1
                                        and play_sound = 1
                                            audio_play_sound(snd_bouncer_idle,50,true);
                                        if abs(can_hover_speed) &gt; 0.5
                                            {
                                                if can_splash_timer = 1
                                                    {
                                                        can_splash_x_0 = x
                                                        can_splash_y_0 = o.water_height
                                                        can_splash_frame_0 = 0
                                                        if play_sound = 1
                                                            audio_play_sound(character_splash,50,false);
                                                    }
                                                if can_splash_timer = 64
                                                    {
                                                        can_splash_x_1 = x
                                                        can_splash_y_1 = o.water_height
                                                        can_splash_frame_1 = 0
                                                        if play_sound = 1
                                                            audio_play_sound(character_splash,50,false);
                                                    }
                                            }
                                        if motor_sound_timer &gt;= 16
                                            {
                                                if abs(can_hover_speed) &gt;= abs(can_hover_target-0.8)
                                                and play_sound = 1
                                                    {
                                                        if !audio_is_playing(snd_bouncer_boating)
                                                            audio_play_sound(snd_bouncer_boating,50,true);
                                                    }
                                                motor_sound_timer = 2
                                                scr_set_screen_shake(12,1)
                                            }
                                        //Bobbing
                                        can_hover_timer += 1
                                        if can_hover_timer &gt; 32
                                            can_hover_timer = 0;
                                        y = lerp(y,o.water_height+32,0.25)
                                        can_hover_speed = lerp(can_hover_speed,can_hover_target,0.1)

                                        //Horizontal Movement
                                        x += can_hover_speed;
                                        var can_hover_speed_cap = 2.4;
                                        if boss_phase_2 = 1
                                            can_hover_speed_cap = 3.4;
                                        if x &gt; o.px can_hover_target = -can_hover_speed_cap;
                                        if x &lt; o.px can_hover_target =  can_hover_speed_cap;
                                    }
                                else
                                    {
                                        can_hover_timer = 16
                                        can_hover_target = 0
                                        can_splash_timer = 0
                                        can_splash_x_0 = -64
                                        can_splash_frame_0 = 0
                                        can_splash_x_1 = -64
                                        can_splash_frame_1 = 0
                                        if motor_sound_timer &gt; 0
                                            {
                                                motor_sound_timer = 0
                                                audio_stop_sound(snd_bouncer_idle)
                                                audio_stop_sound(snd_bouncer_boating)
                                            }
                                    }
                            }
                        //Shake the ground when landing
                        if ground_timer = 1
                            {
                                target_timer = 31
                                target_speed = 8
                                //Hurt The Player If They've Been Punched By The Boxing Glove
                                if (o.hurt[0] = 0 and o.hurt[1] = 0
                                and o.shield_invuln = 32
                                and point_in_circle(x,y,o.universal_player_x,o.universal_player_y,64))
                                    {
                                        if o.user_enemy_level &gt; 1
                                            {
                                                scr_enemy_deal_damage(0.2,0.4,1)
                                                o.hsp = 8*sign(o.hsp)
                                            }
                                    }
                                //Mega Items
                                var instance_ID = collision_circle(x,y,64,obj_mega_item_parent,false,true);
                                if instance_exists(instance_ID)
                                    {
                                        instance_ID.attacked = 2
                                    }
                                //Items
                                var instance_ID = collision_circle(x,y,64,obj_destructible,false,true);
                                if instance_exists(instance_ID)
                                    {
                                        if instance_ID.object_index != obj_bouncer_bomb
                                        and instance_ID.object_index != obj_bouncer_parry_box
                                            instance_ID.attacked = 1;
                                    }
                                //Attack other enemies within range of the boxing glove
                                instance_ID = collision_circle(x,y,64,obj_enemy_parent,false,true);
                                if instance_exists(instance_ID)
                                    {
                                        if instance_ID.attacked = 0 and instance_ID.alpha = 0
                                            {
                                                instance_ID.attacked = 1
                                                instance_ID.attacked_damage = 2
                                            }
                                    }
                                if play_sound = 1
                                    audio_play_sound(snd_heavy_drop,50,false);
                                scr_set_screen_shake(12,1)
                            }
                        if timer = 90
                            {
                                var character_angle = point_direction(body_x,body_y,o.px,o.py);

                                //Leap Forwards
                                if (((character_angle &gt;= 270 or character_angle &lt;= 45) and xdir = -1)
                                or (character_angle &gt; 135 and character_angle &lt; 270 and xdir = 1))
                                and (point_distance(x+16,y,o.universal_player_x,o.universal_player_y) &lt; 72
                                and o.user_enemy_level &gt; 1)
                                    phase = 3;
                                else
                                    {
                                        if body_x &gt; o.px
                                            xdir = -1;
                                        if body_x &lt;= o.px
                                            xdir = 1;
                                        turn = 1
                                    }
                            }
                        //Rotation lag
                        if xdir = -1
                            {
                                if body_angle &lt; 180 body_angle += 4;
                                if body_angle &gt; 180 body_angle -= 4;
                            }
                        if xdir = 1
                            {
                                if body_angle &lt; 0 body_angle += 4;
                                if body_angle &gt; 0 body_angle -= 4;
                            }
                        //Assess which direction to attack the player
                        if timer = 120
                            {
                                var character_angle = point_direction(body_x,body_y,o.px,o.py);
                                if o.user_cursor_mode = 0
                                    {
                                        //Leap Forwards
                                        if ((character_angle &gt;= 270 or character_angle &lt;= 45) and xdir = 1)
                                        or ((character_angle &gt; 135 and character_angle &lt; 270) and xdir = -1)
                                            phase = 2;
                                        //Leap Upwards
                                        if character_angle &gt; 45 and character_angle &lt;= 135
                                            phase = 1;
                                    }
                                timer = 0
                            }
                        break;
                    }
                //Upwards Leaping Phase
                case 1:
                    {
                        var ceiling_check = tile_layer_find(surface_flats,x,y-48);
                        if tile_exists(ceiling_check)
                            {
                                y += 32
                                timer = 0
                                phase = 0
                            }
                        timer += 1
                        if timer = 1
                            {
                                jump_speed = 0
                                jump_target = 10
                                ground_timer = 0
                            }
                        if timer &gt;= 0 and timer &lt; 15
                            {
                                body_y += 5
                                body_angle += 2*-xdir
                            }
                        if timer &gt;= 15 and timer &lt; 45
                            {
                                if (timer = 15)
                                    {
                                        scr_set_screen_shake(12,1)
                                        if play_sound = 1
                                            audio_play_sound(snd_bouncy_cube,50,false);
                                    }
                                if (timer = 20 or timer = 25 or timer = 30)
                                and (play_sound = 1)
                                    audio_play_sound(snd_heavy_skidding,50,false);
                                jump_speed = lerp(jump_speed,jump_target,1)

                                if timer &gt;= 20
                                    {
                                        y -= jump_speed;
                                        gravity_slope = 0
                                    }
                                body_y -= jump_speed

                                body_angle += 2*xdir
                            }
                        if timer = 45
                            {
                                timer = 0
                                phase = 0
                            }
                        break;
                    }
                //Fowards Leaping Phase
                case 2:
                    {
                        var ceiling_check = tile_layer_find(surface_flats,x,y-48);
                        if tile_exists(ceiling_check)
                            {
                                y += 32
                                timer = 0
                                phase = 0
                            }
                        timer += 1
                        if timer = 1
                            {
                                jump_speed = 0
                                jump_target = 10
                                ground_timer = 0
                            }
                        if timer &gt;= 0 and timer &lt; 15
                            {
                                body_y += 5
                                body_angle += 2*-xdir
                            }
                        if timer &gt;= 15 and timer &lt; 45
                            {
                                if (timer = 15)
                                    {
                                        scr_set_screen_shake(12,1)
                                        if play_sound = 1
                                            audio_play_sound(snd_bouncy_cube,50,false);
                                    }
                                if (timer = 20 or timer = 25 or timer = 30)
                                and (play_sound = 1)
                                    audio_play_sound(snd_heavy_skidding,50,false);
                                jump_speed = lerp(jump_speed,jump_target,1)

                                if timer &gt;= 20
                                    {
                                        y -= jump_speed;
                                        gravity_slope = 0
                                    }
                                var wall_id = tile_layer_find(surface_flats,x+(32*xdir),y-32);
                                var forward_speed = jump_speed;
                                if tile_exists(wall_id)
                                    {
                                        if xdir = 1
                                            x = tile_get_x(wall_id)-32;
                                        if xdir = -1
                                            x = tile_get_x(wall_id)+tile_get_width(wall_id)+32;
                                    }
                                else
                                    x += forward_speed*xdir;
                                if x &lt; o.camera_boundary_left     x = o.camera_boundary_left + 32;
                                if x &gt; o.camera_boundary_right-32 x = o.camera_boundary_right - 32;
                                if o.boss_boundary_bottom != -10000
                                and object_index != obj_bouncer_flee
                                    {
                                        if x &lt; o.boss_boundary_left     x = o.boss_boundary_left + 32;
                                        if x &gt; o.boss_boundary_right-32 x = o.boss_boundary_right - 32;
                                    }
                                body_x = x+8
                                body_y -= jump_speed

                                body_angle += 2*xdir
                            }
                        if timer = 45
                            {
                                timer = 0
                                phase = 0
                            }
                        break;
                    }
                //Backwards Bouncing Phase
                case 3:
                    {
                        timer += 1
                        if timer = 1
                            {
                                jump_speed = 0
                                jump_target = 10
                                ground_timer = 0
                                target_timer = 31
                                target_speed = 4
                            }
                        body_x = lerp(body_x,x+(96*-xdir),0.20)
                        body_y = lerp(body_y,y,0.15)
                        body_angle += 3*xdir
                        if body_y &gt; y - 16
                            {
                                //Hurt The Player If They've Been Punched By The Boxing Glove
                                if (o.hurt[0] = 0 and o.hurt[1] = 0
                                and o.shield_invuln = 32
                                and (point_in_circle(body_x,body_y,o.universal_player_x,o.universal_player_y,64)))
                                    scr_enemy_deal_damage(0.2,0.4,1);
                                //Mega Items
                                var instance_ID = collision_circle(body_x,body_y,64,obj_mega_item_parent,false,true);
                                if instance_ID != noone
                                    {
                                        instance_ID.attacked = 2
                                    }
                                //Items
                                var instance_ID = collision_circle(body_x,body_y,64,obj_destructible,false,true);
                                if instance_ID != noone
                                    {
                                        if instance_ID.object_index != obj_bouncer_bomb
                                        and instance_ID.object_index != obj_bouncer_parry_box
                                        instance_ID.attacked = 1
                                    }
                                //Attack other enemies within range of the boxing glove
                                instance_ID = collision_circle(body_x,body_y,64,obj_enemy_parent,false,true);
                                if instance_exists(instance_ID)
                                    {
                                        if instance_ID.attacked = 0 and instance_ID.alpha = 0
                                            {
                                                instance_ID.attacked = 1
                                                instance_ID.attacked_damage = 2
                                            }
                                    }
                                scr_set_screen_shake(12,1)
                                if play_sound = 1
                                    audio_play_sound(snd_heavy_skidding,50,false);
                                timer = 0
                                phase = 0
                            }
                        break;
                    }
            }
        var wall_id = tile_layer_find(surface_flats,x+(32*xdir),y-32);

        if object_index = obj_bouncer_mini_boss and heart &lt; heartmax/2
            wall_id = -1;
        if tile_exists(wall_id)
            {
                if xdir = 1
                    x = tile_get_x(wall_id)-32;
                if xdir = -1
                    x = tile_get_x(wall_id)+tile_get_width(wall_id)+32;
            }
    }
if heart &lt; 0 and o.pause = 0
    {
        if o.timer_stop = 0
            {
                o.timer_stop = 1
                depth = 0
                death = 5
                timer = 0
                x = body_x
                y = body_y
                body_x = 0
                body_y = 0
                body_angle = 0
                if instance_exists(parry_box)
                    instance_destroy(parry_box);
                scr_stop_boss_music()
                if room = rm_campaign
                    {
                        if o.player_bosses[o.player_current] &lt; 2
                            o.player_bosses[o.player_current] = 2;
                    }
            }
        if slowdown_timer = 1 or room = rm_editor
            {
                x += 5*xdir
                y += 2
                body_angle += 2*-xdir
                body_x += 3
                body_y += 1
            }
        gravity_speed = 0
        timer += 1
        death_timer += 1
        o.death[0] = 0
        if timer = 2
            {
                alpha = 1
                if play_sound = 1
                    audio_play_sound(snd_enemy_explosion,50,false);
            }
        if timer &gt;= 8
            {
                timer = 0
                alpha = 1
            }
        slowdown_timer += 1
        if slowdown_timer = 2 and room != rm_editor
            {
                o.pause_logic = 2
                slowdown_timer = 0
            }
        if death_timer &gt; 180
            {
                if room != rm_editor
                    instance_destroy();
                if room = rm_editor
                    {
                        var obj_id = instance_create(start_x,start_y,object_index);
                        obj_id.item = item
                        instance_destroy()
                    }
            }
    }
</string>
