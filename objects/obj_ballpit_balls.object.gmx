                if gravity_slope = 1 or ground = 1
                    {
                        can_timer = 0
                        gravity_slope = 0
                        gravity_speed = -3
                        ground = 0
                        y -= 12
                    }
                subangle += can_speed*2
            }
        if can_timer = 1 and play_sound = 1
            audio_play_sound(can_sound,50,false);
        clamp(can_speed,-4,4)
        x += can_speed
    }
