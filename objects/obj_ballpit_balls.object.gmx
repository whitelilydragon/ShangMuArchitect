            {
                y = ball_id.y-20
                gravity_speed = 0
                ground = 1
            }
        if can_speed !=0 or gravity_speed != 0
            {
                if tile_layer_find(surface_flats,x+8,y-8) can_speed = -4;
                if tile_layer_find(surface_flats,x-8,y-8) can_speed = 4;
                if tile_layer_find(surface_flats,x,y-12) gravity_speed = 4;
            }
        if abs(can_speed) &gt; 0
            {
                if can_speed &gt; 0 can_speed -= 0.06;
                if can_speed &lt; 0 can_speed += 0.06;

                if abs(can_speed) &lt; 0.24
