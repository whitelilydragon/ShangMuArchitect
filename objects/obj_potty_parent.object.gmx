        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Player Interactions
if o.pause = 0
    {
        prompt_flash += 1
        if prompt_flash &gt; 40
            prompt_flash = 0;
        if obj_game_logic.user_cursor_mode = 0
        and point_in_rectangle(o.universal_player_x,o.universal_player_y,x-4,y,x+36,y+64)
            {
                if o.joy_input[0,3] = 1 and abs(o.hsp) &lt; 0.4 and o.ground = 1
                    {
                        if instance_exists(potty_door)
                            {
                                if potty_door.x_scale &gt;= 1
                                    {
                                        hey = 0
                                        o.px = x+16
                                        hold_player = 1
                                        potty_sound = 0
                                        potty_sound_timer = 0
                                        potty_door.door_open = 1
                                    }
                            }
                    }
            }
        if instance_exists(potty_door)
            {
                if (potty_door.door_open = 1)
                    {
                        if (potty_door.door_step = 1 and potty_door.x_scale &gt; 0.8)
                            {
                                if potty_sound = 0
                                    {
                                        depth = -8
                                        potty_sound_timer += 1
                                        //Determine the character's gender and play either a "HEY!!!" sound,
                                        //or a toilet flushing sound effect
                                        if potty_sound_timer = 16
                                            {
                                                //Men's Toilet
                                                if potty_gender = "Male"
                                                    {
                                                        if o.skin = spr_lilac
                                                        or o.skin = spr_carol
                                                        or o.skin = spr_milla
                                                            hey = 1;
                                                        potty_sound_index = snd_male_hey
                                                    }
                                                //Women's Toilet
                                                if potty_gender = "Female"
                                                    {
                                                        if o.skin = spr_spade
                                                            hey = 1;
                                                        potty_sound_index = snd_female_hey_alt
                                                    }
                                                //All-Inclusive Toilet
                                                if potty_gender = "Trans"
                                                    hey = 0;
                                                //Fully replenish stamina if we're using the correct toilet
                                                if hey = 0
                                                    {
                                                        if o.stamina &lt; 100
                                                            {
                                                                o.stamina = 100
                                                                o.stamina_boosted = 0
                                                            }
                                                        audio_play_sound(snd_toilet_flush,50,false);
                                                    }
                                                if hey = 1
                                                    audio_play_sound(potty_sound_index,50,false);
                                            }
                                        if potty_sound_timer &gt; 32
                                            {
                                                depth = 0
                                                potty_sound = 1
                                                hold_player = 0
                                                potty_sound_timer = 0
                                                potty_door.door_open = 1
                                                potty_door.door_step = 0
                                                potty_door.x_scale_target = -0.75
                                                if obj_game_logic.user_cursor_mode = 0
                                                and point_in_rectangle(o.universal_player_x,o.universal_player_y,x-4,y,x+36,y+64)
                                                    {
                                                        if (potty_gender = "Female" and o.skin = spr_spade)
                                                            {
                                                                audio_play_sound(snd_frying_pan,50,false)
                                                                audio_play_sound(snd_pan_hit,50,false)
                                                                potty_groan_timer = 48
                                                            }
                                                        if hey = 1
                                                            {
                                                                audio_play_sound(character_spring_weak,50,false)
                                                                o.hsp = 6;
                                                            }
                                                    }
                                            }
                                        
                                    }
                            }
                    }
            }
        if keyboard_check(vk_anykey) or mouse_check_button(mb_any)
            npc_interface_switch_control_type = 0;
        //Check Controller's Action Buttons
        if gamepad_button_check(o.joy_modern_id,o.joy_up_mod) = 1
            npc_interface_switch_control_type = 1;

        if hold_player = 1
            {
                o.px = x+16
                o.py = y+48
            }
        if potty_groan_timer &gt; 0
            {
                potty_groan_timer -= 1
                if potty_groan_timer = 1
                    audio_play_sound(snd_trans_hey,50,false);
            }
    }
</string>
