    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Logic and Animations
event_inherited();

if o.pause = 0 and freeze = 0
    { 
        if death = 0
            {
                switch(state)
                    {
                        //Idle
                        case 0:
                            {
                                //Animations
                                frame += 0.18;
                                if frame &gt; 4.8 frame = 0;
                                //Iterate Attack State
                                being += 1;
                                if item = spr_mod_chip_speed being += 3;
                                if being &gt;= 118
                                    {
                                        if o.user_cursor_mode = 0
                                            {
                                                state = 1
                                                frame = 0
                                                sprite_index = spr_ring_hammer_attack
                                            }
                                        else
                                            being = 0;
                                    }
                                //Reset Hitbox State
                                bullet_timer = -1
                                break;
                            }
                        //Attack
                        case 1:
                            {
                                frame += 0.35
                                if frame &gt; 18.8
                                    {
                                        frame = 0
                                        being = 0
                                        state = 0
                                        sound = 0
                                        sprite_index = spr_ring_hammer_idle
                                    }
                                if frame &gt; 16 and sound = 0
                                    {
                                        sound = 1
                                        bullet_x = x
                                        bullet_y = y
                                        bullet_timer = 0
                                        bullet_frame = 0
                                        bullet_speed = 0
                                        bullet_active = 1
                                        if play_sound = 1
                                            {
                                                audio_play_sound(snd_enemy_destroyed,50,false)
                                                audio_play_sound(snd_mud_splat,50,false)
                                                scr_set_screen_shake(6,3)
                                            }
                                        //Create Rock Particles If Our Performance Profile Is Set To "Normal"
                                        if o.performance_profile = 1 and instance_number(obj_destructible_particle) &lt; 16
                                            {
                                                scr_attack_shield_ext(x,y+16,obj_destructible_particle,0,spr_rock_broken_0,-4  ,1,1,-6)
                                                scr_attack_shield_ext(x,y+16,obj_destructible_particle,0,spr_rock_broken_0,-2,1,1,-7.25)
                                                scr_attack_shield_ext(x,y+16,obj_destructible_particle,0,spr_rock_broken_0,-1  ,1,1,-7.625)
                                                scr_attack_shield_ext(x,y+16,obj_destructible_particle,0,spr_rock_broken_0,0   ,1,1,-9.5)
                                                scr_attack_shield_ext(x,y+16,obj_destructible_particle,0,spr_rock_broken_0,2,1,1,-8)
                                            }
                                    }
                            }
                    }
            }
        //Bullet Projectile
        if bullet_active = 1
            {
                var enemy_surface_id = -1;
                var enemy_bullet_id  = -1;
                var enemy_ramp_confirm = 0;

                if (bullet_timer = 0
                and obj_game_logic.hurt[0] = 0 and obj_game_logic.hurt[1] = 0
                and obj_game_logic.shield_invuln = 32
                and point_distance(bullet_x,bullet_y,obj_game_logic.universal_player_x,obj_game_logic.universal_player_y) &lt; 38)
                    {
                        bullet_timer = 1
                        enemy_explosion_x = bullet_x
                        enemy_explosion_y = bullet_y
                        enemy_explosion_frame = 0
                        scr_enemy_deal_damage(0.4,1,2)
                        if play_sound = 1 audio_play_sound(snd_enemy_projectile,50,false);
                        scr_set_screen_shake(24,3)
                    }
                if bullet_scanner_timer &gt; 3 and bullet_timer = 0
                    {
                        //Bomb Cube
                        var instance_ID = collision_circle(bullet_x,bullet_y,32,obj_player_cube_alt,false,true);
                        if instance_exists(instance_ID)
                            {
                                if instance_ID.cube_jiggle_timer = 0
                                    {
                                        instance_ID.cube_jiggle_timer = 10
                                        instance_ID.cube_health += 0.50
                                        bullet_timer = 1
                                        enemy_explosion_x = bullet_x
                                        enemy_explosion_y = bullet_y
                                        enemy_explosion_frame = 0
                                        scr_set_screen_shake(24,3)
                                    }
                            }
                        if obj_game_logic.user_enemy_level &lt; 2
                            {
                                //Mega Items
                                var instance_ID = collision_circle(bullet_x,bullet_y,32,obj_mega_item_parent,false,true);
                                if instance_exists(instance_ID)
                                    {
                                        instance_ID.attacked = 2
                                        bullet_timer = 1
                                        enemy_explosion_x = bullet_x
                                        enemy_explosion_y = bullet_y
                                        enemy_explosion_frame = 0
                                        scr_set_screen_shake(24,3)
                                    }
                                if room != rm_editor
                                    {
                                        var crate_ID = collision_point(bullet_x,bullet_y,obj_destructible,false,true);
                                        if instance_exists(crate_ID)
                                            {
                                                bullet_timer = 1
                                                crate_ID.attacked = 1
                                                enemy_explosion_x = bullet_x
                                                enemy_explosion_y = bullet_y
                                                enemy_explosion_frame = 0
                                                if play_sound = 1 audio_play_sound(snd_enemy_projectile,50,false);
                                                scr_set_screen_shake(24,3)
                                            }
                                    }
                                //Attack other enemies within range of the boxing glove
                                instance_ID = collision_circle(bullet_x,bullet_y,32,obj_enemy_parent,false,true);
                                if instance_exists(instance_ID)
                                    {
                                        if instance_ID.attacked = 0 and instance_ID.alpha = 0
                                            {
                                                instance_ID.attacked = 1
                                                instance_ID.attacked_damage = 2
                                                bullet_timer = 1
                                                enemy_explosion_x = bullet_x
                                                enemy_explosion_y = bullet_y
                                                enemy_explosion_frame = 0
                                                scr_set_screen_shake(24,3)
                                            }
                                    }
                            }
                    }
                //Bullet Explosion
                if bullet_timer = 1
                    {
                        enemy_explosion_frame += 0.24
                        if enemy_explosion_frame &gt; 7.8
                            {
                                bullet_x = 0
                                bullet_y = 0
                                bullet_timer = 0
                                bullet_frame = 0
                                bullet_speed = 0
                                bullet_active = 0
                                enemy_explosion_frame = 0
                            }
                    }
            }
    }
</string>
          </argument>
        </arguments>
      </action>
    </event>
