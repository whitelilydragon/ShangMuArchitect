            <string>if obj_game_logic.pause = 0
    {
        //Sound Playback Conditions
        var play_sound = 1;
        if x &lt; view_xview[view_port]-196
        or x &gt; view_xview[view_port]+view_wview[view_port]+196
            play_sound = 0;
        if y &lt; view_yview[view_port]-196
        or y &gt; view_yview[view_port]+view_hview[view_port]+196
            play_sound = 0;

        //Floating
        state = 0

        if point_in_rectangle(o.px,o.py,x+0,y,x+68,y+48)
            state = state_right;

        //Tipping Towards The Right
        var count = 0;
        while(count &lt;= segment_count)
            {
                if state = 0
                    {
                        segment_y[count] -= (segment_cap[count]);
                        if segment_y[count] &lt; 0
                            segment_y[count] = 0;
                    }
                if state = state_right
                    {
                        //Tilt
                        segment_acc[count] += (segment_cap[count]);
                        
                    }
                if state = state_left
                    {
                        //Tilt
                        segment_acc[count] -= (segment_cap[count]);
                    }

                if segment_acc[count] &gt; segment_cap[count]
                    segment_acc[count] = segment_cap[count];
                if segment_acc[count] &lt; -segment_cap[count]
                    segment_acc[count] = -segment_cap[count];
                
                if (segment_y[4] &gt; segment_dip_bottom+2 and state = state_right)
                or (segment_y[4] &lt; segment_dip_bottom+2 and state = state_left)
                    segment_acc[count] = 0;

                //Apply Speed
                if state &gt; 0
                    segment_y[count] += segment_acc[count];

                count += 1
            }
        var count = 0;
        while(count &lt;= segment_count)
            {
                //Update Collision Mask Positions
                if tile_exists(tile[count])
                    tile_set_position(tile[count],x+segment_x[count],y+16+segment_y[count]);
                count += 1
            }
        if point_in_rectangle(o.px,o.py,x-12,y,x+32,y+48)
        and o.ground = 1 and spring_activate = 0
            spring_activate = 1;

        if point_in_rectangle(o.px,o.py,x+24,y,x+64,y+48)
        and segment_y[4] &gt;= segment_dip_bottom
            spring_activate = -1;
        if spring_activate = -1
            {
                if o.ground = 0
                or o.px &lt; x-12 or o.px &gt; x+64
                    spring_activate = 0;
            }

        if spring_activate = 1
            {
                if point_in_rectangle(o.px,o.py,x-12,y,x+32,y+48)
                and o.ground = 1
                and o.spring_rotate != spring_rotated
                and o.user_cursor_mode = 0
                and segment_y[4] &gt;= segment_dip_bottom
                    {
                        scr_set_spring_launch(10.7,spring_timer,spring_rotated)
                        o.gravity_speed -= 10.7
                        o.anim_current = o.anim_spring
                        spring_activate = 0
                        audio_play_sound(snd_diving_board,50,false)
                    }
            }
    }
