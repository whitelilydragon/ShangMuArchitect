                                        use_corner_box_bottom_left_y-use_tile_height-(use_tile_height*use_corner_counter_y),
                                        user_anim_tile_object);

                                        anim_tile_id.tile_id = corner_tile_id
                                        tile_set_alpha(corner_tile_id,0)
