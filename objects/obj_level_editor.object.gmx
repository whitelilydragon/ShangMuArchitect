                                                if user_tile_scan_x &gt;= user_grid_snap_x+user_grid_offset_x+background_get_width(user_tile_selected)-user_tile_scan_distance
                                                    {
                                                        user_tile_scan_x = user_grid_snap_x+user_grid_offset_x
                                                        user_tile_scan_y += 4
                                                    }
                                            }
                                    }
                                if user_tile_valid = 1
