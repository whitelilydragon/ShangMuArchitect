                                use_corner_box_bottom_left_x-use_tile_width-(use_tile_width*use_corner_counter_x),
                                use_corner_box_bottom_left_y-use_tile_height-(use_tile_height*use_corner_counter_y),
                                user_layer_selected)
                                if object_exists(user_anim_tile_object)
                                    {
