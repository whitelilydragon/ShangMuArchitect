                                        use_corner_counter_y += 1;
                                    }
                            }
                    }
                //Delete duplicate tile originally spawned by the place tool
                if tile_exists(added_tile)
                    {
                        tile_delete(added_tile)
                        added_tile = -1
                    }

                if array_length_1d(tile_get_ids()) &gt; user_tile_limit_warning_value
