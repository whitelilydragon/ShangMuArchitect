                                                use_corner_box = 2;
                                                use_corner_box_id = o;
                                                use_corner_box_bottom_left_x = tile_get_x(o)
                                                use_corner_box_bottom_left_y = tile_get_y(o)
                                                use_corner_box_top_right_x = user_grid_snap_x
                                                use_corner_box_top_right_y = user_grid_snap_y
