        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if room = rm_editor
    {
        if o.user_cursor_mode = 0
            {
                if (o.debug_hud != 1 and visible = false) visible = true;
                if (o.debug_hud = 1  and visible = true) visible = false;
            }
        else
            visible = true;

        edit_timer += 1

        if edit_timer &gt; 30
            {
                var instance_ID = instance_position(x,y,obj_destructible);
                
                if instance_exists(instance_ID)
                    {
                        if variable_instance_exists(instance_ID,"rotate_angle")
                            {
                                if object_index = obj_angle_gear_0   instance_ID.rotate_angle = 0;
                                if object_index = obj_angle_gear_90  instance_ID.rotate_angle = 90;
                                if object_index = obj_angle_gear_180 instance_ID.rotate_angle = 180;
                                if object_index = obj_angle_gear_270 instance_ID.rotate_angle = 270;
                                if room != rm_editor instance_destroy();
                            }
                        else
                            {
                                scr_set_message("Object cannot be rotated!")
                                instance_destroy()
                            }
                    }
                edit_timer = 0
            }

        var parent = obj_level_editor;
        //Display FOF Overlay If Hovered Over
        if (point_in_rectangle(parent.use_cursor_x,parent.use_cursor_y,x,y-54,x+32,y)
        and parent.use_tile_menu = 0 and parent.side_bar_allow = 0
        and ((parent.user_cursor_display_mode = 1 and parent.use_tile_switching_mode = 1) or parent.use_tile_switching_mode = 0)
        )
            fof_overlay_enable = 1;

        if fof_overlay_cooldown &gt; 0
            fof_overlay_cooldown -= 1;

        if fof_overlay_enable = 1
            {
                if parent.side_bar_allow = 0
                    parent.user_place_allow = 1;
                if !point_in_rectangle(parent.use_cursor_x,parent.use_cursor_y,x,y-54,x+32,y)
                or parent.side_bar_allow = 1 or parent.use_tile_menu &gt; 0
                    {
                        fof_overlay_enable = 0
                        parent.user_place_allow = 0
                    }
                if (mouse_check_button_pressed(parent.joy_mouse_button_left) or parent.use_put_here = 1)
                and parent.use_cursor_y &lt; y-8 and fof_overlay_cooldown = 0
                    {
                        fof_overaly_iteration += 1
                        if fof_overaly_iteration &gt; 3
                            fof_overaly_iteration = 0;
                        audio_play_sound(snd_turn_page,50,false)
                    }
                if (mouse_check_button_pressed(parent.joy_mouse_button_right) or parent.use_delete = 1)
                and parent.use_cursor_y &lt; y-8 and fof_overlay_cooldown = 0
                    {
                        fof_overaly_iteration -= 1
                        if fof_overaly_iteration &lt; 0
                            fof_overaly_iteration = 3;
                        audio_play_sound(snd_turn_page,50,false)
                    }
                switch(fof_overaly_iteration)
                    {
                        case  0: {instance_change(obj_angle_gear_0,true); break;}
                        case  1: {instance_change(obj_angle_gear_90,true); break;}
                        case  2: {instance_change(obj_angle_gear_180,true); break;}
                        case  3: {instance_change(obj_angle_gear_270,true); break;}
                    }
            }
    }
</string>
