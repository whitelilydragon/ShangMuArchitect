        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Logic and Detonation

//Pause Safety Check
if o.pause = 0
{

if point_distance(x+56,y+56,o.universal_player_x,o.universal_player_y) &lt; 96
    {
        if detonate_detected = 0 and o.user_cursor_mode = 0
        and o.ground = 1
            {
                audio_stop_sound(snd_petal_shatter)
                audio_play_sound(snd_petal_shatter,50,false)
                audio_stop_sound(snd_shock)
                audio_play_sound(snd_shock,50,false)
                detonate_detected = 1;
            }
    }

//If We Have Not Yet Detonated, Play Animations System
if detonate_detected = 1
    {
        //Advance Frames
        if o.user_enemy_level &lt;= 2
            frame += 0.35;
        if o.user_enemy_level &gt; 2
            frame += 0.5;
        if frame &gt; 12.8
            {
                detonate_detected = 0
                frame = 0
            }
        if frame &gt;= 8
            {
                //Deal 4 Points Of Damage To The Player If They Were Too Close
                if point_distance(x+56,y+56,o.universal_player_x,o.universal_player_y) &lt; 96
                and o.ground = 1
                and o.hurt[0] = 0
                    {
                        scr_enemy_deal_damage(1,2,4);
                        audio_stop_sound(snd_shock)
                        audio_play_sound(snd_shock,50,false)
                        scr_set_screen_shake(14,2)
                    }
            }
    }

if room = rm_editor and tile_id != -100
    {
        if !tile_exists(tile_id)
            instance_destroy();
    }
} //End of Pause Safety Check
</string>
